{
    "manifest_version": 3,
    "name": "Auto Enhancer",
    "version": "1.1",
    "description": "Automatically interacts with webpages, including YouTube.",
    "permissions": ["storage", "tabs"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content4.js", "content5.js", "content6.js"]
        }
    ]
}
